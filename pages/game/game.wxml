<!--pages/game/game.wxml-->
<view class="auto">
    <image style="width:100%;height:100%;" mode='aspectFill' src="http://m.qpic.cn/psc?/54881c6b-985b-436f-a0ab-4079de7ce009/45NBuzDIW489QBoVep5mcRI.bDacbIQl*EGRUc4Jc.OFR7vz699wClNHd7HhJ1or1pm5ZnAuxd*.mSCv*xMLRRnj9mAQLJxskQfM5tXdqDo!/b&bo=aAEMAwAAAAADJ2Q!&rf=viewer_4"></image>
</view>


<view class="container">
    <button class="text1" bindtap="toBegin1" size="mini">退出房间</button>
</view>

<open-data type="userAvatarUrl" class="image1"></open-data>
<open-data type="userNickName" class="container1" style="color: rgb(255, 234, 234);"></open-data>

<view class="container2">
    <view class="point1">
        <image src="{{one_image}}"></image>
        <image src="{{two_image}}"></image>
    </view>

    <view class="point2">
        <image src="{{three_image}}"></image>
        <image src="{{four_image}}"></image>
    </view>
    <view class="point3">
        <image src="{{five_image}}"></image>
        <image src="{{six_image}}"></image>
    </view>
    
    <button class="button" style="width:40%" size="mini" bindtap="go">来一发!</button>

    <view class="msg" style="color: rgb(245, 11, 11);">恭喜你！{{msg}}</view>
    
</view>